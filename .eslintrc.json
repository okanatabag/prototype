{
	"env": {
		"browser": true,
		"es2021": true,
		"jest": true,
		"node": true
	},
	"extends": ["airbnb", "prettier"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"linebreak-style": ["error", "windows"],
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
	}
}
